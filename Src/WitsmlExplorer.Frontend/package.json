    "@microsoft/signalr": "^7.0.0",
    "@tanstack/react-table": "^8.9.2",
    "@tanstack/react-virtual": "^3.0.0-beta.54",
    "ace-builds": "^1.29.0",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
